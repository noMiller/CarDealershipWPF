<Page x:Class="WpfApp4.Pages.Admin.Edit.EditClientsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp4.Pages.Admin.Edit"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EditClientsPage" Background="White">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Изменение клиента" FontSize="25" Margin="0 0 0 20"/>
            <TextBlock Text="ФИО"/>
            <TextBox x:Name="FIOTextBox"/>
            <TextBlock Text="ID аккаунта"/>
            <TextBox x:Name="IDuserTextBox"/>
            <Button x:Name="EditClientButton" Click="EditClientButton_Click" Content="Изменить клиента" Margin="0 20 0 0" Height="30"/>
        </StackPanel>
    </Grid>
</Page>
